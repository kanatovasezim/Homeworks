<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>myServlet</servlet-name>
        <servlet-class>com.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myServlet</servlet-name>
        <url-pattern>/numbers</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletTime</servlet-name>
        <servlet-class>com.ServletTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletTime</servlet-name>
        <url-pattern>/time</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletSum</servlet-name>
        <servlet-class>com.ServletSum</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletSum</servlet-name>
        <url-pattern>/sum</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletRedirect</servlet-name>
        <servlet-class>com.ServletRedirect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRedirect</servlet-name>
        <url-pattern>/gotoGoogle</url-pattern>
    </servlet-mapping>
</web-app>
